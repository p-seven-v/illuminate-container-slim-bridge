{
    "name": "p7v/illuminate-container-slim-bridge",
    "description": "Illuminate (Laravel) Container integration in Slim",
    "keywords": ["slim", "container", "illuminate", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@p7v.app"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0 || ~8.0.0 || ~8.1.0",
        "slim/slim": "^4.2.0",
        "php-di/invoker": "^2.0",
        "illuminate/container": "^7.23 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "P7v\\IlluminateContainerSlim\\": "src/"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.6"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
